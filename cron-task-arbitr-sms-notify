#!/bin/sh
# atrun uid=1000 gid=1000
# mail node 0
umask 2
CLUTTER_IM_MODULE=xim; export CLUTTER_IM_MODULE
COLORFGBG=15\;0; export COLORFGBG
DBUS_SESSION_BUS_ADDRESS=unix:path=/run/user/1000/bus; export DBUS_SESSION_BUS_ADDRESS
DEFAULTS_PATH=/usr/share/gconf/ubuntu.default.path; export DEFAULTS_PATH
DERBY_HOME=/usr/lib/jvm/java-8-oracle/db; export DERBY_HOME
DESKTOP_SESSION=ubuntu; export DESKTOP_SESSION
GDMSESSION=ubuntu; export GDMSESSION
GIO_LAUNCHED_DESKTOP_FILE=/usr/share/applications/qterminal.desktop; export GIO_LAUNCHED_DESKTOP_FILE
GIO_LAUNCHED_DESKTOP_FILE_PID=2574; export GIO_LAUNCHED_DESKTOP_FILE_PID
GJS_DEBUG_OUTPUT=stderr; export GJS_DEBUG_OUTPUT
GJS_DEBUG_TOPICS=JS\ ERROR\;JS\ LOG; export GJS_DEBUG_TOPICS
GNOME_DESKTOP_SESSION_ID=this-is-deprecated; export GNOME_DESKTOP_SESSION_ID
GNOME_SHELL_SESSION_MODE=ubuntu; export GNOME_SHELL_SESSION_MODE
GPG_AGENT_INFO=/run/user/1000/gnupg/S.gpg-agent:0:1; export GPG_AGENT_INFO
GTK_IM_MODULE=ibus; export GTK_IM_MODULE
GTK_MODULES=gail:atk-bridge; export GTK_MODULES
HOME=/home/node; export HOME
IM_CONFIG_PHASE=2; export IM_CONFIG_PHASE
J2REDIR=/usr/lib/jvm/java-8-oracle/jre; export J2REDIR
J2SDKDIR=/usr/lib/jvm/java-8-oracle; export J2SDKDIR
JAVA_HOME=/usr/lib/jvm/java-8-oracle; export JAVA_HOME
LANG=ru_RU.UTF-8; export LANG
LANGUAGE=ru:en; export LANGUAGE
LC_ADDRESS=ru_RU.UTF-8; export LC_ADDRESS
LC_IDENTIFICATION=ru_RU.UTF-8; export LC_IDENTIFICATION
LC_MEASUREMENT=ru_RU.UTF-8; export LC_MEASUREMENT
LC_MONETARY=ru_RU.UTF-8; export LC_MONETARY
LC_NAME=ru_RU.UTF-8; export LC_NAME
LC_NUMERIC=ru_RU.UTF-8; export LC_NUMERIC
LC_PAPER=ru_RU.UTF-8; export LC_PAPER
LC_TELEPHONE=ru_RU.UTF-8; export LC_TELEPHONE
LC_TIME=ru_RU.UTF-8; export LC_TIME
LIBVIRT_DEFAULT_URI=qemu:///system; export LIBVIRT_DEFAULT_URI
LOGNAME=node; export LOGNAME
MANDATORY_PATH=/usr/share/gconf/ubuntu.mandatory.path; export MANDATORY_PATH
PATH=/home/node/.npm-global/bin:/home/node/.local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:/snap/bin:/usr/lib/jvm/java-8-oracle/bin:/usr/lib/jvm/java-8-oracle/db/bin:/usr/lib/jvm/java-8-oracle/jre/bin; export PATH
PWD=/home/node; export PWD
QT4_IM_MODULE=xim; export QT4_IM_MODULE
QT_ACCESSIBILITY=1; export QT_ACCESSIBILITY
QT_IM_MODULE=ibus; export QT_IM_MODULE
SESSION_MANAGER=local/k8s:@/tmp/.ICE-unix/6391,unix/k8s:/tmp/.ICE-unix/6391; export SESSION_MANAGER
SHLVL=1; export SHLVL
SSH_AGENT_PID=11239; export SSH_AGENT_PID
SSH_AUTH_SOCK=/tmp/ssh-IH5xhBg1vdod/agent.6391; export SSH_AUTH_SOCK
S_COLORS=auto; export S_COLORS
TEXTDOMAIN=gdm; export TEXTDOMAIN
TEXTDOMAINDIR=/usr/share/locale/; export TEXTDOMAINDIR
USER=node; export USER
USERNAME=node; export USERNAME
WINDOWID=0; export WINDOWID
WINDOWPATH=1; export WINDOWPATH
XAUTHORITY=/run/user/1000/gdm/Xauthority; export XAUTHORITY
XDG_CONFIG_DIRS=/etc/xdg/xdg-ubuntu:/etc/xdg; export XDG_CONFIG_DIRS
XDG_CURRENT_DESKTOP=ubuntu:GNOME; export XDG_CURRENT_DESKTOP
XDG_DATA_DIRS=/usr/share/ubuntu:/usr/local/share:/usr/share:/var/lib/snapd/desktop; export XDG_DATA_DIRS
XDG_MENU_PREFIX=gnome-; export XDG_MENU_PREFIX
XDG_RUNTIME_DIR=/run/user/1000; export XDG_RUNTIME_DIR
XDG_SEAT=seat0; export XDG_SEAT
XDG_SESSION_DESKTOP=ubuntu; export XDG_SESSION_DESKTOP
XDG_SESSION_ID=1; export XDG_SESSION_ID
XDG_SESSION_TYPE=x11; export XDG_SESSION_TYPE
XDG_VTNR=1; export XDG_VTNR
XMODIFIERS=@im=ibus; export XMODIFIERS
OLDPWD=/home/node; export OLDPWD
ZSH=/home/node/.oh-my-zsh; export ZSH
UPDATE_ZSH_DAYS=13; export UPDATE_ZSH_DAYS
PAGER=less; export PAGER
LESS=-R; export LESS
LC_CTYPE=en_US.UTF-8; export LC_CTYPE
LSCOLORS=Gxfxcxdxbxegedabagacad; export LSCOLORS
LS_COLORS=rs=0:di=01\;34:ln=01\;36:mh=00:pi=40\;33:so=01\;35:do=01\;35:bd=40\;33\;01:cd=40\;33\;01:or=40\;31\;01:mi=00:su=37\;41:sg=30\;43:ca=30\;41:tw=30\;42:ow=34\;42:st=37\;44:ex=01\;32:\*.tar=01\;31:\*.tgz=01\;31:\*.arc=01\;31:\*.arj=01\;31:\*.taz=01\;31:\*.lha=01\;31:\*.lz4=01\;31:\*.lzh=01\;31:\*.lzma=01\;31:\*.tlz=01\;31:\*.txz=01\;31:\*.tzo=01\;31:\*.t7z=01\;31:\*.zip=01\;31:\*.z=01\;31:\*.Z=01\;31:\*.dz=01\;31:\*.gz=01\;31:\*.lrz=01\;31:\*.lz=01\;31:\*.lzo=01\;31:\*.xz=01\;31:\*.zst=01\;31:\*.tzst=01\;31:\*.bz2=01\;31:\*.bz=01\;31:\*.tbz=01\;31:\*.tbz2=01\;31:\*.tz=01\;31:\*.deb=01\;31:\*.rpm=01\;31:\*.jar=01\;31:\*.war=01\;31:\*.ear=01\;31:\*.sar=01\;31:\*.rar=01\;31:\*.alz=01\;31:\*.ace=01\;31:\*.zoo=01\;31:\*.cpio=01\;31:\*.7z=01\;31:\*.rz=01\;31:\*.cab=01\;31:\*.wim=01\;31:\*.swm=01\;31:\*.dwm=01\;31:\*.esd=01\;31:\*.jpg=01\;35:\*.jpeg=01\;35:\*.mjpg=01\;35:\*.mjpeg=01\;35:\*.gif=01\;35:\*.bmp=01\;35:\*.pbm=01\;35:\*.pgm=01\;35:\*.ppm=01\;35:\*.tga=01\;35:\*.xbm=01\;35:\*.xpm=01\;35:\*.tif=01\;35:\*.tiff=01\;35:\*.png=01\;35:\*.svg=01\;35:\*.svgz=01\;35:\*.mng=01\;35:\*.pcx=01\;35:\*.mov=01\;35:\*.mpg=01\;35:\*.mpeg=01\;35:\*.m2v=01\;35:\*.mkv=01\;35:\*.webm=01\;35:\*.ogm=01\;35:\*.mp4=01\;35:\*.m4v=01\;35:\*.mp4v=01\;35:\*.vob=01\;35:\*.qt=01\;35:\*.nuv=01\;35:\*.wmv=01\;35:\*.asf=01\;35:\*.rm=01\;35:\*.rmvb=01\;35:\*.flc=01\;35:\*.avi=01\;35:\*.fli=01\;35:\*.flv=01\;35:\*.gl=01\;35:\*.dl=01\;35:\*.xcf=01\;35:\*.xwd=01\;35:\*.yuv=01\;35:\*.cgm=01\;35:\*.emf=01\;35:\*.ogv=01\;35:\*.ogx=01\;35:\*.aac=00\;36:\*.au=00\;36:\*.flac=00\;36:\*.m4a=00\;36:\*.mid=00\;36:\*.midi=00\;36:\*.mka=00\;36:\*.mp3=00\;36:\*.mpc=00\;36:\*.ogg=00\;36:\*.ra=00\;36:\*.wav=00\;36:\*.oga=00\;36:\*.opus=00\;36:\*.spx=00\;36:\*.xspf=00\;36:; export LS_COLORS
EDITOR=vim; export EDITOR
ARCHFLAGS=-arch\ x86_64; export ARCHFLAGS
SSH_KEY_PATH=\~/.ssh/rsa_id; export SSH_KEY_PATH
cd /home/node || {
         echo 'Execution directory inaccessible' >&2
         exit 1
}
